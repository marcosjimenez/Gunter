<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Gunter.UI.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Gunter.UI.MAUI.Pages">

    <FlyoutItem Title="Main" Icon="main.png">
        <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
    </FlyoutItem>

    <FlyoutItem Title="Options" Icon="config.png">
        <ShellContent ContentTemplate="{DataTemplate local:ConfigPage}" />
    </FlyoutItem>
    
</Shell>
